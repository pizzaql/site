{
    "version": 2,
    "routes": [
      {
        "src": "/", 
        "headers": {
          "cache-control": "s-maxage=2628000, maxage=0",
          "accept-encoding": "br"
        }
      },
      {	
        "src": "^/service-worker.js$",	
        "dest": "/_next/static/service-worker.js",
        "headers": {
          "cache-control": "public, max-age=43200, immutable",
          "Service-Worker-Allowed": "/"
        }
      }	
    ],
    "builds": [
      {
        "src": "next.config.js",
        "use": "@now/next"
      }
    ],
    "regions": ["all"]
}